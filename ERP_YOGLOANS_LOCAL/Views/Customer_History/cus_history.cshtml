  function kyc_grid() {
    $.ajax({
        url: '@Url.Action("kyc_grid", "Customer_History")',
        type: 'GET',
        success: function (response) {

            if (response.success) {
                var data = response.data;
                var tableBody = $('#kycTable tbody');
                tableBody.empty(); // Clear any existing rows

                data.forEach(function(row) {
                    var tr = $('<tr></tr>');
                    var td1 = $('<td></td>').text(row.Column1);
                    var td2 = $('<td></td>').text(row.Column2);
                    var td3 = $('<td></td>').text(row.Column3); // New column for kyc_id
                    $("#kyc_grid_div").show();
                    // Create the Attachments column with an icon
                    var td4 = $('<td></td>');
                    var icon = $('<i></i>')
                        .addClass('fa fa-paperclip')
                        .css({ 'font-size': '17px', 'color': 'maroon', 'cursor': 'pointer' }) // Make the icon look clickable
                        .attr('data-kyc-id', row.Column3); // Store the kyc_id in a data attribute

                    // Attach a click event handler to the icon
                    icon.on('click', function() {
                        var kycId = $(this).data('kyc-id');
                      //  alert('Icon clicked for KYC ID: ' + kycId);
                        callKycGridAttachment(kycId);

                        // Example: Open a modal, download a file, etc.
                        // openDocumentModal(kycId); // Example function call
                    });

                    td4.append(icon);

                    // Append all cells to the row
                    tr.append(td1);
                    tr.append(td2);
                   // tr.append(td3); // Append the new kyc_id column
                    tr.append(td4);

                    // Append the row to the table body
                    tableBody.append(tr);
                });
            } else {
                console.error("Failed to load KYC grid data");
            }
        },
        error: function(xhr, status, error) {
            console.error("Error fetching data: ", error);
        }
    });
}


function Bank_grid() {
    $.ajax({
        url: '@Url.Action("Bankdtl_grid", "Customer_History")',
        type: 'GET',
        success: function (response) {
            if (response.success) {
                var data = response.data;
                var tableBody = $('#BankTable tbody');
                tableBody.empty(); // Clear any existing rows

                data.forEach(function(row) {
                    var tr = $('<tr></tr>');
                    var td1 = $('<td></td>').text(row.Column1); // Acc No
                    var td2 = $('<td></td>').text(row.Column2); // IFSC
                    var td3 = $('<td></td>').text(row.Column3); // Bank
                    var td4 = $('<td></td>').text(row.Column4); // Branch

                    // Create the attachment icon
                    var td5 = $('<td></td>');
                    var attachmentIcon = $('<i style="font-size:17px;color:maroon;"></i>').addClass('fa fa-paperclip').css('cursor', 'pointer');
                    attachmentIcon.click(function() {
                        // Handle the attachment click event
                        viewAttachment(); // Pass any relevant data
                    });
                    td5.append(attachmentIcon);

                    // Append all columns to the row
                    tr.append(td1);
