        $(document).ready(function () {
            $('#button_1').on('click', function () {
                var buttonValue = $(this).data('value');

                $.ajax({
                    url: '/Customer_History/history_tables_select',
                    type: 'GET',
                    data: { buttonValue: buttonValue },
                    success: function (response) {
                        if (response.success) {
                            $('#tablesContainerWrapper').show();
                            $('html, body').animate({
                                scrollTop: $("#tablesContainerWrapper").offset().top
                            }, 1000);
                            var $tablesContainer = $('#tablesContainer');

                            if ($tablesContainer.length === 0) {
                                console.error('#tablesContainer not found');
                                return;
                            }

                            $tablesContainer.empty();

                            // Define custom headings and their desired order
                            var tableOrder = [
                                { name: 'Table1', heading: 'Live' },
                                { name: 'Table', heading: 'Settled' },
                                { name: 'Table2', heading: 'Due' }
                            ];

                            // Create a sorted array of table data based on the desired order
                            var sortedTables = tableOrder.map(function (item) {
                                return {
                                    tableName: item.name,
                                    heading: item.heading,
                                    data: response.tables[item.name]
                                };
                            }).filter(function (item) {
                                return item.data && item.data.Rows && item.data.Rows.length > 0; // Only include tables that have data
                            });

                            // Generate and append HTML for each table
                            $.each(sortedTables, function (_, table) {
                                if (table.data && table.data.Rows.length > 0) {
                                    var tableHtml = '<div><h3 class="table-heading">' + table.heading + '</h3><table border="1" style="width:100%; font-size:12px; font-weight:500;"><thead><tr>';

                                    // Add table headers
                                    $.each(table.data.Rows[0], function (key) {
                                        tableHtml += '<th>' + key + '</th>';
                                    });
                                    tableHtml += '</tr></thead><tbody>';

                                    // Add all table rows
                                    $.each(table.data.Rows, function (_, row) {
                                        tableHtml += '<tr>';

                                        // Track column index manually
                                        var colIndex = 0;

                                        // Iterate through each cell in the row
                                        $.each(row, function (_, value) {
                                            // Check if it's the second column (colIndex === 1)
                                            if (colIndex === 1) {  // Right-align the second column
                                                tableHtml += '<td style="text-align: right;">' + value + '</td>';
                                            } else {
                                                tableHtml += '<td>' + value + '</td>';
                                            }
                                            colIndex++; // Increment the column index
                                        });
                                        tableHtml += '</tr>';
                                    });
                                    tableHtml += '</tbody></table></div>';

                                    // Append the table to the container
                                    $tablesContainer.append(tableHtml);
                                }
                            });
                        } else {
                            alert("Failed to retrieve data. " + response.message);
                        }
                    },
                    error: function (xhr, status, error) {
                        console.error('Error:', error);
                        console.error('Status:', status);
                        console.error('Response Text:', xhr.responseText);
                        alert("An error occurred. Check the console for details.");
                    }
                });
            });
        });




