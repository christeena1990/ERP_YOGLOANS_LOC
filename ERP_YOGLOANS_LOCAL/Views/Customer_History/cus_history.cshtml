function cust_photo() {
    $.ajax({
        url: '@Url.Action("GetCustomerPhoto", "Customer_History")',
        type: 'GET',
        dataType: 'json',
        success: function(response) {
            if (response.success === false) {
                $('#customerPhoto').attr('src', '');
                $("#customerPhoto").empty().hide();
            } else {
                $('#customerPhoto').attr('src', 'data:image/png;base64,' + response.photo).show();
                $('#popupImage').attr('src', 'data:image/png;base64,' + response.photo); // Set popup image
            }
        },
        error: function(xhr, status, error) {
            console.error("Error fetching customer photo:", error);
        }
    });

    // Show popup on mouse over
    $('#customerPhoto').hover(
        function() {
            $('#photoPopup').css({
                display: 'block',
                top: $(this).offset().top + $(this).height() + 5, // Position popup below image
                left: $(this).offset().left
            });
        },
        function() {
            $('#photoPopup').hide(); // Hide popup on mouse out
        }
    );
}



