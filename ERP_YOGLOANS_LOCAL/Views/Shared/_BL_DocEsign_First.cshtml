                                        @String.Join(", ", coApplicant1Address)
                                    }
                                </td>
                                <td style="border: 1px solid #ddd; width: 25%;">
                                    @{
                                        var coApplicant2Address = new List<string>(); // Create a list to store non-null address parts

                                        // Conditionally add each part to the list if not null or empty
                                        if (!string.IsNullOrEmpty(Model.ApplicationFormData.CoApplicant2_Address1)) { coApplicant2Address.Add(Model.ApplicationFormData.CoApplicant2_Address1); }
                                        if (!string.IsNullOrEmpty(Model.ApplicationFormData.CoApplicant2_Address2)) { coApplicant2Address.Add(Model.ApplicationFormData.CoApplicant2_Address2); }
                                        if (!string.IsNullOrEmpty(Model.ApplicationFormData.CoApplicant2_Address3)) { coApplicant2Address.Add(Model.ApplicationFormData.CoApplicant2_Address3); }
                                        if (!string.IsNullOrEmpty(Model.ApplicationFormData.CoApplicant2_Address4)) { coApplicant2Address.Add(Model.ApplicationFormData.CoApplicant2_Address4); }
                                        if (!string.IsNullOrEmpty(Model.ApplicationFormData.CoApplicant2_Post)) { coApplicant2Address.Add(Model.ApplicationFormData.CoApplicant2_Post); }
                                        if (!string.IsNullOrEmpty(Model.ApplicationFormData.CoApplicant2_City)) { coApplicant2Address.Add(Model.ApplicationFormData.CoApplicant2_City); }
                                        if (!string.IsNullOrEmpty(Model.ApplicationFormData.CoApplicant2_State)) { coApplicant2Address.Add(Model.ApplicationFormData.CoApplicant2_State); }

                                        // Join the address parts with commas and output them
                                        @String.Join(", ", coApplicant2Address)
                                    }
                                </td>
                                <td style="border: 1px solid #ddd;">
                                    @{
                                        var coApplicant3Address = new List<string>(); // Create a list to store non-null address parts

                                        // Conditionally add each part to the list if not null or empty
                                        if (!string.IsNullOrEmpty(Model.ApplicationFormData.CoApplicant3_Address1)) { coApplicant3Address.Add(Model.ApplicationFormData.CoApplicant3_Address1); }
                                        if (!string.IsNullOrEmpty(Model.ApplicationFormData.CoApplicant3_Address2)) { coApplicant3Address.Add(Model.ApplicationFormData.CoApplicant3_Address2); }
                                        if (!string.IsNullOrEmpty(Model.ApplicationFormData.CoApplicant3_Address3)) { coApplicant3Address.Add(Model.ApplicationFormData.CoApplicant3_Address3); }
                                        if (!string.IsNullOrEmpty(Model.ApplicationFormData.CoApplicant3_Address4)) { coApplicant3Address.Add(Model.ApplicationFormData.CoApplicant3_Address4); }
