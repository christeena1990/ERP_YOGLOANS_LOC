                                <td style="border: 1px solid #ddd; width: 25%;">@Model.ApplicationFormData.CoApplicant2_Email</td>
                                <td style="border: 1px solid #ddd;">@Model.ApplicationFormData.CoApplicant3_Email</td>

                            </tr>

                            <tr>
                                <td style="text-align: left; padding: 4px; border: 1px solid #ddd; width: 26%;">Physical Address /<br />മേൽവിലാസം</td>
                                <td style="border: 1px solid #ddd; width: 25%;">
                                    @{
                                        var coApplicant1Address = new List<string>(); // Create a list to store non-null address parts

                                        // Conditionally add each part to the list if not null or empty
                                        if (!string.IsNullOrEmpty(Model.ApplicationFormData.CoApplicant1_Address1)) { coApplicant1Address.Add(Model.ApplicationFormData.CoApplicant1_Address1); }
                                        if (!string.IsNullOrEmpty(Model.ApplicationFormData.CoApplicant1_Address2)) { coApplicant1Address.Add(Model.ApplicationFormData.CoApplicant1_Address2); }
