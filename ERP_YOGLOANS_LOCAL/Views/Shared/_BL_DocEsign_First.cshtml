
@{
    ViewBag.Title = "_BL_DocEsign_First";
}

@*@model ERP_YOGLOANS_LOCAL.Models.Legality_Model.Leegality_DataFill_Models*@

@model ERP_YOGLOANS_LOCAL.Models.Legality_Model.ApplicationAndKFSViewModel

@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>

        body {
            font-family: 'Times New Roman';
            /*font-family: 'Arial', sans-serif;*/
            margin: 0;
            padding: 0;
            position: relative;
        }

        .page {
            page-break-after: always;
            min-height: calc(100vh - 50px); /* Adjust to ensure enough space for footer */
            margin-bottom: 50px; /* Space to prevent overlapping */
            padding-bottom: 60px; /* Add padding to avoid content overlapping with the footer */
        }

        .footer {
            position: absolute; /* Keeps the footer at the bottom */
            left: 0;
            bottom: 0;
            width: 100%;
            text-align: center;
            font-size: 12px;
            border-top: 1px solid #000;
            padding: 5px; /* Increase padding for better visibility */
            background-color: white;
            height: 30px; /* Adjust height to make it visible */
        }

        .content {
            padding: 20px;
            font-size: 10px;
        }

        .company-info {
            text-align: center;
            font-weight: bold;
            margin-bottom: 5px;
        }

        .application-title {
            text-align: center;
            margin-top: 20px;
            margin-bottom: 5px;
        }
        /* -------------------KFS------------------------------------*/

        .heading-container {
            margin: 10px 0;
        }

        table {
            border-collapse: collapse;
            border: 1px solid #ddd;
            width: 100%; /* Adjust width to 100% */
            margin: 5px auto; /* Reduce margin */
        }

        th, td {
            border: 1px solid #ddd;
            padding: 8px; /* Reduced padding */
            text-align: left;
            font-size: 10px;
        }

        .small-column {
            width: 20px; /* Adjusted width */
            text-align: center;
        }

        .large-column {
            width: 20%;
        }

        .largest-column {
            width: 300px;
        }

        .pdf-button {
            margin: 10px;
            padding: 5px 10px;
            font-size: 12px;
            color: white;
            background-color: #007bff;
            border: none;
            border-radius: 5px;
            cursor: pointer;
        }

        .large-column {
            word-wrap: break-word; /* Allows long words to break */
            white-space: normal; /* Allows text to wrap onto the next line */
            overflow-wrap: break-word; /* Ensures long words break as necessary */
        }

        .no-space {
            border-collapse: collapse; /* Optional: removes space between cells */
        }

        td {
            padding: 8px; /* Adds some padding for better readability */
            vertical-align: top;
        }


        hr {
            border: 0;
            height: 1px;
            background-color: #ADB1B1;
            margin: 20px 0; /* Adjust margin for spacing */
        }

        .page-break {
            page-break-before: always;
            break-before: page;
        }

        table, .content {
            page-break-inside: auto; /* Avoid page breaks within the table or content block */
        }

        tr, td {
            page-break-inside: avoid;
        }

        p, li {
            page-break-inside: avoid;
            break-inside: avoid;
        }
    </style>


</head>
<body>

    <div class="container">
        <div class="page">
            <div class="outline-container" id="pdf-content">
                <div id="applictn_form">
                    <div class="logo-src" style="margin-bottom: 5px; text-align:right;">
                        <img style="width:170px;" src="~/images/logowithtext.png" alt="Company Logo" />
                    </div>

                    <div class="company-info" style="margin-bottom: -20px; line-height: 1.2;">
                        <div style="font-size:12px;">യോഗക്ഷേമം ലോൺസ് ലിമിറ്റഡ്</div>
                        <div style="font-size:12px;">YOGAKSHEMAM LOANS LIMITED</div>
                        <div style="font-size:11px; font-weight:normal;">(INCORPORATED UNDER THE COMPANIES ACT 1956)</div>
                        <div style="font-size:11px;">CIN: U65992KL1991PLC005965</div>
                        <div style="font-size: 11px; font-weight: normal;">Regd. Office: 3rd Floor, Ottappath Tower, Aswini Junction, Thiruvambadi PO., Thrissur, Kerala - 680 022<br />APPLICATION FOR BUSINESS / PROPERTY LOAN</div><br />
                    </div>
                    <hr />

                    <div class="application-title" style="margin-top: -5px; margin-bottom: 10px; line-height: 1.2; font-size: 12px; font-weight: bold;">
                        APPLICATION FOR BUSINESS / PROPERTY LOAN<br />
                        <span class="malayalam" style="font-weight: bold;">ബിസിനസ്സ്/വസ്തു വായ്പാ അപേക്ഷ</span>
                    </div>

                    <div class="toaddress" style="line-height: 1.2; margin-bottom: 1px; font-size:10px; margin-left:14px;">
                        To,<br />
                        The Manager / മാനേജർ<br />
                        Yogakshemam Loans Ltd. / യോഗക്ഷേമം ലോൺസ് ലിമിറ്റഡ്<br />
                        @Model.ApplicationFormData.BranchName
                    </div>
                    <div class="content">

                        <p style="margin-bottom:-10px">
                            Sir / Madam / സർ / മാഡം,<br />
                            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; I/We request you to sanction business/property loan of Rs&nbsp;@Model.ApplicationFormData.AppliedLoanAmt (Rupees @Model.ApplicationFormData.AppliedLoanAmtWords) My/our personal / profession / business details are furnished below.<br />
                            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;എനിയ്ക്ക് / ഞങ്ങൾക്ക്&nbsp;@Model.ApplicationFormData.AppliedLoanAmt &nbsp; രൂപയുടെ ( &nbsp;@Model.ApplicationFormData.AppliedLoanAmtWords&nbsp;രൂപ)

                            ബിസിനസ്സ് / പ്രോപ്പർട്ടി ലോൺ അനുവദിക്കുവാൻ അപേക്ഷിക്കുന്നു.
                        </p><br />
                        <p style="margin-bottom:-5px; text-align:center;">
                            എൻ്റെ / ഞങ്ങളുടെ / വ്യക്തിപരവും / തൊഴിൽപരവുമായ വിശദാംശങ്ങൾ താഴെ ചേർക്കുന്നു.
                        </p>
                        <table style="width: 100%; border-collapse: collapse; margin-top: 20px; margin-bottom:7px;" id="applicationTable">
                            <tr>
                                <td style="text-align: left; padding: 4px; border: 1px solid #ddd; width: 40%;">
                                    Name of the Loan Applicant<br />അപേക്ഷകന്റെ പേര്
                                </td>
                                <td colspan="2" style="border: 1px solid #ddd; width: 60%;">@Model.ApplicationFormData.PrimaryApplicantName</td>
                            </tr>
                            <tr>
                                <td style="text-align: left; padding: 4px; border: 1px solid #ddd; width: 40%;">Son of/ Daughter of / Wife of<br />അച്ഛന്റേയോ / അമ്മയുടേയോ / <br />ഭർത്താവിന്റേയോ പേര്</td>
                                <td colspan="2" style="border: 1px solid #ddd; width: 60%;" @*id="D_S_W_Name"*@>@Model.ApplicationFormData.PrimaryApplicantGuardianType,@Model.ApplicationFormData.PrimaryApplicantGuardian</td>
                            </tr>

                            <tr>
                                <td style="text-align: left; padding: 4px; border: 1px solid #ddd; width: 40%;">Sex / ലിംഗം</td>
                                <td style="border: 1px solid #ddd; width: 30%;">
                                    Male / ആണ്&nbsp;&nbsp;  <input type="checkbox" id="genderMale" style="width: 20px; height: 20px;"
                                                                   @(Model.ApplicationFormData.PrimaryApplicantGender == "Male" ? "checked" : "")>
                                </td>
                                <td style="border: 1px solid #ddd; width: 30%;">
                                    Female / പെണ്ണ് &nbsp;&nbsp;<input type="checkbox" id="genderFemale" style="width: 20px; height: 20px;"
                                                                       @(Model.ApplicationFormData.PrimaryApplicantGender == "Female" ? "checked" : "")>
                                </td>
                            </tr>


                            <tr>
                                <td style="text-align: left; padding: 4px; border: 1px solid #ddd; width: 40%;">Age and Date of Birth<br />വയസ്സ്, ജനനതിയതി</td>
                                <td colspan="2" style="border: 1px solid #ddd; width: 60%;" id="DOB">@Model.ApplicationFormData.PrimaryApplicantAgeDOB</td>
                            </tr>
                            <tr>
                                <td style="text-align: left; padding: 4px; border: 1px solid #ddd; width: 40%;">
                                    Mobile No (+91)<br />മൊബൈൽ നമ്പർ
                                </td>
                                <td colspan="2" style="border: 1px solid #ddd; width: 60%;" id="mobileNoCell">@Model.ApplicationFormData.Mobile</td>
                            </tr>
                            <tr>
                                <td style="text-align: left; padding: 4px; border: 1px solid #ddd; width: 40%;">Whatsapp No (+91)<br />വാട്ട്സ്ആപ്പ് നമ്പർ</td>
                                <td colspan="2" style="border: 1px solid #ddd; width: 60%;" id="whtsappno"></td>
                            </tr>
                            <tr style="margin-bottom:20px;">
                                <td style="text-align: left; padding: 4px; border: 1px solid #ddd; width: 40%;">Email ID<br /> ഇ-മെയിൽ വിലാസം</td>
                                <td colspan="2" style="border: 1px solid #ddd; width: 60%;" id="email_id"></td>
                            </tr>
                            <tr>
                                <td style="text-align: left; padding: 4px; border: 1px solid #ddd; width: 40%; ">
                                    Present Residential Address<br />ഇപ്പോഴത്തെ മേൽവിലാസം
                                </td>
                                <td colspan="2" style="border: 1px solid #ddd; text-align: center; width: 60%;" id="present_address">
                                    @{
                                        var address = new List<string>(); // Create a list to store non-null address parts

                                        if (!string.IsNullOrEmpty(Model.ApplicationFormData.PrimaryApplicantPresentAddress1)) { address.Add(Model.ApplicationFormData.PrimaryApplicantPresentAddress1); }
                                        if (!string.IsNullOrEmpty(Model.ApplicationFormData.PrimaryApplicantPresentAddress2)) { address.Add(Model.ApplicationFormData.PrimaryApplicantPresentAddress2); }
                                        if (!string.IsNullOrEmpty(Model.ApplicationFormData.PrimaryApplicantPresentAddress3)) { address.Add(Model.ApplicationFormData.PrimaryApplicantPresentAddress3); }
                                        if (!string.IsNullOrEmpty(Model.ApplicationFormData.PrimaryApplicantPresentAddress4)) { address.Add(Model.ApplicationFormData.PrimaryApplicantPresentAddress4); }
                                        if (!string.IsNullOrEmpty(Model.ApplicationFormData.PrimaryApplicantPresentCity)) { address.Add(Model.ApplicationFormData.PrimaryApplicantPresentCity); }
                                        if (!string.IsNullOrEmpty(Model.ApplicationFormData.PrimaryApplicantPresentPost)) { address.Add(Model.ApplicationFormData.PrimaryApplicantPresentPost); }
                                        if (!string.IsNullOrEmpty(Model.ApplicationFormData.PrimaryApplicantPresentDistrict)) { address.Add(Model.ApplicationFormData.PrimaryApplicantPresentDistrict); }
                                        if (!string.IsNullOrEmpty(Model.ApplicationFormData.PrimaryApplicantPresentState)) { address.Add(Model.ApplicationFormData.PrimaryApplicantPresentState); }

                                        // Join the address parts with commas and output them
                                        @String.Join(", ", address)
                                    }
                                    @*<div style="display: flex; justify-content: left; align-items: center; height: 100%;">
                                <span style="display: block; margin-top: 5px;">PIN Code/പിൻകോഡ്</span>&nbsp;
                                <input type="text" maxlength="6" style="width: 120px; text-align: center; border: 1px solid #ddd; height: 30px; margin-left: 10px;" id="present_pin" />
                            </div>*@
                                    <div style="display: flex; justify-content: left; align-items: center; height: 100%;">
                                        <span style="display: block; margin-top: 5px;">PIN Code/പിൻകോഡ്</span>&nbsp;
                                        <!-- Bind the input field to the PrimaryApplicantPresentPinCode model property -->
                                        <input type="text" maxlength="6"
                                               style="width: 120px; text-align: center; border: 1px solid #ddd; height: 30px; margin-left: 10px;"
                                               id="present_pin"
                                               name="PrimaryApplicantPresentPinCode"
                                               value="@Model.ApplicationFormData.PrimaryApplicantPresentPinCode" />
                                    </div>

                                </td>
                            </tr>






                            <tr>
                                <td style="text-align: left; padding: 4px; border: 1px solid #ddd; width: 40%; ">
                                    Permanent Residential Address <br />സ്ഥിരം മേൽവിലാസം
                                </td>
                                <td colspan="2" style="border: 1px solid #ddd; text-align: center; width: 60%;" id="perma_address">
                                    @{
                                        var permanentAddress = new List<string>(); // Create a list to store non-null address parts

                                        // Conditionally add each part to the list if not null or empty
                                        if (!string.IsNullOrEmpty(Model.ApplicationFormData.PrimaryApplicantPermanentAddress1)) { permanentAddress.Add(Model.ApplicationFormData.PrimaryApplicantPermanentAddress1); }
                                        if (!string.IsNullOrEmpty(Model.ApplicationFormData.PrimaryApplicantPermanentAddress2)) { permanentAddress.Add(Model.ApplicationFormData.PrimaryApplicantPermanentAddress2); }
                                        if (!string.IsNullOrEmpty(Model.ApplicationFormData.PrimaryApplicantPermanentAddress3)) { permanentAddress.Add(Model.ApplicationFormData.PrimaryApplicantPermanentAddress3); }
                                        if (!string.IsNullOrEmpty(Model.ApplicationFormData.PrimaryApplicantPermanentCity)) { permanentAddress.Add(Model.ApplicationFormData.PrimaryApplicantPermanentCity); }
                                        if (!string.IsNullOrEmpty(Model.ApplicationFormData.PrimaryApplicantPermanentPost)) { permanentAddress.Add(Model.ApplicationFormData.PrimaryApplicantPermanentPost); }
                                        if (!string.IsNullOrEmpty(Model.ApplicationFormData.PrimaryApplicantPermanentDistrict)) { permanentAddress.Add(Model.ApplicationFormData.PrimaryApplicantPermanentDistrict); }
                                        if (!string.IsNullOrEmpty(Model.ApplicationFormData.PrimaryApplicantPermanentState)) { permanentAddress.Add(Model.ApplicationFormData.PrimaryApplicantPermanentState); }

                                        // Join the address parts with commas and output them
                                        @String.Join(", ", permanentAddress)
                                    }

                                    <div style="display: flex; justify-content: left; align-items: center; height: 100%;">
                                        <span style="display: block; margin-top: 5px;">PIN Code/പിൻകോഡ്</span>&nbsp;
                                        <input type="text" maxlength="6"
                                               style="width: 120px; text-align: center; border: 1px solid #ddd; height: 30px; margin-left: 10px;"
                                               id="present_pin"
                                               name="PrimaryApplicantPresentPinCode"
                                               value="@Model.ApplicationFormData.PrimaryApplicantPermanentPinCode" />
                                    </div>
                                </td>

                            </tr>
                        </table>

                        <p style="font-weight:bold;">Particulars of Co-applicant/ അപേക്ഷകരുടെ വിവരങ്ങൾ</p>
                        <table style="width: 100%;  border-collapse: collapse; margin-top: 20px; margin-bottom:7px;">
                            <tr>
                                <td style="text-align: left; padding: 4px; border: 1px solid #ddd; width: 26%;">Name/പേര്</td>
                                <td style="border: 1px solid #ddd; width: 25%;">@Model.ApplicationFormData.CoApplicant1_Name</td>
                                <td style="border: 1px solid #ddd; width: 25%;">@Model.ApplicationFormData.CoApplicant2_Name</td>
                                <td style="border: 1px solid #ddd;">@Model.ApplicationFormData.CoApplicant3_Name</td>
                            </tr>
                            <tr>
                                <td style="text-align: left; padding: 4px; border: 1px solid #ddd; width: 26%;">Age & Date of Birth /<br />വയസ്സ്, ജനന തീയതി</td>
                                <td style="border: 1px solid #ddd; width: 25%;">@Model.ApplicationFormData.CoApplicant1_AgeDOB</td>
