                <td>@Model.BenchmarkRate</td>
                <td>@Mode</td>
                <td>@Model.Spread</td>
                <td>@Model.FinalRate</td>
                <td>@Model.ResetPeriodB</td>
                <td>@Model.ResetPeriodS</td>
                <td>@Model.BenchEPI</td>
                <td>@Model.BenchNoEPI</td>
            </tr>
        </table>

        <table id="content5" class="no-space">
            <tr>
                <td class="small-column" style="font-weight:bold">8</td>
                <td class="largest-column" colspan="2" style="font-weight:bold;">Fee/ Charges4</td>
            </tr>
        </table>

        <table id="combined-table" class="no-space">
