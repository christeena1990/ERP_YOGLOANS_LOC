                <td>@Model.SwitchingCharge</td>
            </tr>
            <tr>
                <td class="small-column">v)</td>
                <td class="largest-column">Any other charges (please specify)</td>
                <td>@Model.OtherCharge</td>
            </tr>
        </table>


        @*</div>*@

        @*<div class="heading-container">*@
        <div class="heading-container page-break">
