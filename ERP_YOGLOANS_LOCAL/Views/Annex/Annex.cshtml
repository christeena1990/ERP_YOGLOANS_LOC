    <style>
        html {
            zoom: 85%; /* Adjust this percentage to scale down the content */
        }

        /* Ensure a new page starts here */
        .page-break {
            page-break-before: always; /* Start on a new page */
            break-before: page; /* For better compatibility */
        }

        body {
            font-family: Arial, sans-serif;
            margin: 0;
            padding: 0;
            text-align: center;
            font-size: 12px; /* Smaller font size for better fitting */
        }

        .heading-container {
            margin: 10px 0;
        }

            .heading-container h1 {
                font-size: 18px; /* Adjust font size */
                margin: 5px 0;
            }

            .heading-container h2 {
                font-size: 14px; /* Adjust font size */
                margin: 3px 0;
            }

        table {
            border-collapse: collapse;
            border: 1px solid #ddd;
            width: 100%; /* Adjust width to 100% */
            margin: 5px auto; /* Reduce margin */
        }

        th, td {
            border: 1px solid #ddd;
            padding: 8px; /* Reduced padding */
            text-align: left;
        }

        .small-column {
            width: 20px; /* Adjusted width */
            text-align: center;
        }

        .large-column {
            width: 150px; /* Adjusted width */
        }

        .largest-column {
            width: 300px; /* Adjusted width */
        }

        .pdf-button {
            margin: 10px;
            padding: 5px 10px;
            font-size: 12px;
            color: white;
            background-color: #007bff;
            border: none;
            border-radius: 5px;
            cursor: pointer;
        }
    </style>



</head>
<body>
    <div id="annex_a">
        <div class="heading-container">
            <h1>Key Facts Statement</h1>
            <h2>Part 1 (Interest rate and fees/charges)</h2>
        </div>

        <!-- Container for centered tables -->
        @*<div class="table-container">*@
        <!-- Main Table -->
        <table id="content1" class="no-space">
            <tr>
                <td class="small-column" style="font-weight:bold">1</td>
                <td class="large-column" style="font-weight:bold">Loan proposal/ account No:&nbsp; @Model.ApplicationId</td>
                <td class="large-column" style="font-weight:bold">Type of Loan:&nbsp; @Model.LoanType</td>
            </tr>
            <tr>
                <td class="small-column" style="font-weight:bold">2</td>
                <td class="large-column" style="font-weight:bold">Sanctioned Loan amount (in Rupees)</td>
                <td class="large-column">@Model.LoanAmount</td>
            </tr>
            <tr>
                <td class="small-column" style="font-weight:bold">3</td>
                <td class="large-column">
                    <span class="disbursal-schedule">Disbursal schedule</span><br>
                    <span class="disbursal-details">
                        (i) &nbsp; in stages or 100% upfront.<br>
                        (ii) &nbsp; If it is stage wise, mention the clause of loan<br /> agreement having relevant details
                    </span>
                </td>
                <td class="large-column"></td>
            </tr>
            <tr>
                <td class="small-column" style="font-weight:bold">4</td>
                <td class="large-column" style="font-weight:bold">Loan term (year/months/days)</td>
                <td class="large-column">@Model.Duration</td>
            </tr>
            <tr>
                <td class="small-column" style="font-weight:bold">5</td>
                <td class="largest-column" colspan="2" style="font-weight:bold;">Instalment details</td>
            </tr>
        </table>

        <!-- Nested Table -->
        <table id="content2" class="no-space">
            <thead>
                <tr>
                    <th style="width:50px">Type of instalments</th>
                    <th style="width:50px">Number of EPIs</th>
                    <th style="width:50px">EPI (₹)</th>
                    <th style="width:50px">Commencement of repayment, post sanction</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>@Model.Instalment</td>
                    <td>@Model.NumberOfEPIs</td>
                    <td>@Model.EPI</td>
                    <td>@Model.CommencementOfRepayment</td>
                </tr>
                <!-- Add more rows as needed -->
            </tbody>
        </table>

        <table id="content3" class="no-space">
            <tr>
                <td class="small-column" style="font-weight:bold">6</td>
