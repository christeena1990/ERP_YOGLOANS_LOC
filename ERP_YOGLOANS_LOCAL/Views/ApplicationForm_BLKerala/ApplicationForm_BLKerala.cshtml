<script>
  function fetchData(applicationId) {
    $.ajax({
        url: '@Url.Action("Fillform", "ApplicationForm_BLKerala")',
        type: 'POST',
        data: { applicationId: applicationId },
        success: function (response) {
            if (response.success) {
                console.log("AJAX Response:", response.data); // Log the full response

                // Accessing data for specific tables
                const applicationFormData = response.data.ApplicationForm; // Adjust according to your table naming
                const sanctionLetterData = response.data.SanctionLetter; // Adjust according to your table naming
                // Repeat for other tables...

                if (applicationFormData && applicationFormData.length > 0) {


                    // Populate table with data
                    document.getElementById("loanApplicantCell").innerText = applicationFormData[0].PrimaryApplicantName || 'Not Provided';
                    document.getElementById("D_S_W_Name").innerText = applicationFormData[0].PrimaryApplicantGuardian || 'Not Provided'; // Add other fields similarly
                    document.getElementById("DOB").innerText = applicationFormData[0].PrimaryApplicantAgeDOB || 'Not Provided'; // Age and Date of Birth
                    document.getElementById("mobileNoCell").innerText = applicationFormData[0].CoApplicant1_Mobile || 'Not Provided';
                    document.getElementById("whtsappno").innerText = applicationFormData[0].WhatsappNo || 'Not Provided'; // WhatsApp Number
                    document.getElementById("email_id").innerText = applicationFormData[0].EmailID || 'Not Provided'; // Email ID
                    document.getElementById("present_address").innerText = applicationFormData[0].PrimaryApplicantPresentAddress1 || 'Not Provided'; // Present Address
                    document.getElementById("perma_address").innerText = applicationFormData[0].PrimaryApplicantPermanentAddress2 || 'Not Provided'; // Permanent Address

                    if (applicationFormData[0].PrimaryApplicantGender=== 'Male') {
                        document.getElementById("genderMale").checked = true;
                    } else if (applicationFormData[0].PrimaryApplicantGender === 'Female') {
                        document.getElementById("genderFemale").checked = true;
                    }

                } else {
                    alert("No application data found.");
                }
            } else {
                alert(response.message || "Failed to fetch data.");
            }
        },
        error: function (xhr, status, error) {
            console.error("Error occurred:", error);
            alert("An error occurred while fetching data: " + error + "\nStatus: " + status + "\nResponse Text: " + xhr.responseText);
        }
    });
}

</script>

